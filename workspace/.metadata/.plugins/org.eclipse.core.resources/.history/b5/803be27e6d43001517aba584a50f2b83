
public class Dog extends Animal {

	private boolean wagging_tail = false;
	public Dog(int in_numLegs, boolean in_hungry, String in_size) {
		super(in_numLegs, in_hungry, in_size);
	}
    
	public void wagTail (){
		wagging_tail = true;
	}
	
	public boolean isTailWagging (){
		return wagging_tail;
	}
}
